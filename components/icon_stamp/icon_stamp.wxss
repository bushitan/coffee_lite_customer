
/******集点数*******/
.dian{
    margin:15px 0 0;
}
.score_title{
    color: #efaf30;
    text-align: center;
    font-size: 25pt;
}
.score{
    position: relative;
    display: flex;
    justify-content: center;
}
.score .icon{
    /* width: 40%; */
    width: 150px;
    height: 64px;
    z-index: 999999;
}
.score .water{
    position: absolute;
    bottom: 0;
    height: 64px;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items:flex-end;
    z-index: -99999;
    /* left: 30%; */
}
.score .water .bg{
    width: 140px;
    background-color: #efaf30;
    margin-bottom: 2px;
}

.rate{
    color:#1d2a6d;
    text-align: center;
    font-size: 20pt;
    font-weight: bolder;
}
.rate label{
    color:#efaf30;
}


.stamp_list{
    display: flex;

    justify-content: flex-start;
    flex-wrap:wrap;
}
.node{
    margin-bottom:8px; 
    width: 25%;
    flex-basis:25%;
    display: flex;
    justify-content: center;
}
.icon{
    width: 64px;
    height: 64px;
}