<view  class='back' wx:if="{{nav==NAV_REDIRECT}}" catchtap='back'>
    <image src='../../images/back.png'></image>
</view>

 <view class="page">
    
    <!-- <official-account></official-account> -->
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
                <icon type="success" size="93" color='#efaf30' wx:if="{{status == STATUS_SUCCESS}}"></icon>
                <icon type="warn" size="93"wx:else></icon>   
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{title}}</view>
            <view class="weui-msg__desc">{{content}}</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" catchtap='back' style='background-color:#efaf30;'>返回店铺</button>
                <!-- <button class="weui-btn" type="default"  catchtap='more'>查看更多美食资讯</button> -->
            </view>
        </view>
        
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button  open-type="contact">有疑问？联系客服</button>
            </view>
        </view>
    </view>
